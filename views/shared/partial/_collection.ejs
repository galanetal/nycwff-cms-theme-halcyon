<div class="row">
	<div class="col-12 col-sm-12 col-lg-12">
		<h1><a href="/collection/<%= collection.name %>"><%= collection.title %></a></h1>
		<div id="carousel-example-generic" class="carousel slide" data-ride="carousel" style="margin-bottom: 5em;">
		
		

		  <!-- Wrapper for slides -->
		      <% var metaauthor = collection.primaryauthor;
						var metadate = collection.createdat; %>
					<div class="content">
					<% if(typeof collection.content!='undefined'){ %>
					<%= collection.content %>
					<% } %>
					</div>
		    <% if(collection.items && collection.items.length>0){
				collection.items.forEach(function(item,index){%>
						<div class="item">
							<h2><%= item.item.title %></h2>


							<div class="content"><%- item.item.content %></div>
							
							<% include _itemmeta %>
						</div>
					<%});
				}
				for(var i in collection.items){
					if(typeof collection.items[i].title!='undefined'){

				%>

						<div class="item">
							<h2><%= collection.items[i].title %></h2>
							<div class="content"><%- collection.items[i].content %></div>
						</div>
				<%
					}
				}
				%>

		</div>

	</div>
</div>